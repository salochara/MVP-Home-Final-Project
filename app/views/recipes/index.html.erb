<h1>3 Recipes that Match your preferences:</h1>

<% @recipes.each do |recipe| %>
<div class="recipe-card">
  <div class="recipe-image">
    <%= image_tag(recipe.image_url) %>
  </div>
  <div class="recipe name">
  <%= link_to recipe.name, recipe.url %>
  </div>

  <%= recipe.url %>


</div>
  <% end %>
